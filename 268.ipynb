   "source": [
    "class Solution:\n",
    "    def missingNumber(self, nums: List[int]) -> int:\n",
    "        a = set(range(len(nums) + 1))\n",
    "        a = list(a - set(nums))\n",
    "        \n",
    "        return a[0]"
   ]
