   "source": [
    "class Solution:\n",
    "    def mySqrt(self, x: int) -> int:\n",
    "\n",
    "        num = x / 2\n",
    "        while abs((num * num) -  x) > 0.1:\n",
    "            num = (num **2 + x) / (2 * num)\n",
    "            \n",
    "        return int(num)\n",
    "\n",
    "Solution().mySqrt(36)"
   ]
  }
