   "source": [
    "class Solution:\n",
    "    def majorityElement(self, nums: List[int]) -> int:\n",
    "\n",
    "        # return statistics.mode(nums)\n",
    "        \n",
    "        nums.sort()\n",
    "        return nums[round((len(nums)-1)/2)]"
   ]
